<capptain>
  <agent>
    <!-- Your application id. -->
    <applicationId>ReplaceWithYourApplicationId</applicationId>

    <!-- Your sdk key. -->
    <SDKKey>ReplaceWithYourSDKKey</SDKKey>

    <!-- [Optional] Indicate whether the agent have to report crach or not. -->
    <reportCrash>true</reportCrash>
  </agent>
  <reach>
    <!-- [Optional] Indicate whether the native push (MPNS) is activated or not. -->
    <enableNativePush>true</enableNativePush>

    <!-- [Optional] Name of the push channel. Provide the same that your application may currently use. -->
    <channelName></channelName>
  </reach>
</capptain>